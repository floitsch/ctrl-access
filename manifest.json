{
  "name": "ctrl-access",
  "version": "0.4.2",
  "description": "Navigate to links with a trigger key (by default the ctrl-key).",
  "icons": { "48": "icon48.png",
             "128": "icon128.png" },
  "permissions": ["tabs"],
  "background_page": "bg.html",
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["ctrl-access.js"],
    "css": ["ctrl-access.css"],
    "run_at": "document_end",
    "all_frames": true
  }
  ],
  "options_page": "options.html"
}
